{
  "initial": {
    "html": "/data/sample.html",
    "js": "/data/sample.js"
  },
  "componentsTree": {
    "name": "YS UI",
    "subNodes": [

      {
        "name": "Layout",
        "subNodes": [

          {
            "name": "Common",
            "subNodes": [

              {
                "name": "Wrapper",
                "markup": "<div class=\"ys-ui-wrapper\"></div>"
              },

              {
                "name": "Group",
                "markup": "<div class=\"\"></div>",
                "parameters": [

                  {
                    "name": "Groupping direction",
                    "attr": "class",
                    "type": "select",
                    "possibleValues": [
                      { "label": "Vertical", "value": "ys-ui-group-vertical" },
                      { "label": "Horizontal", "value": "ys-ui-group-horizontal" }
                    ],
                    "value": "ys-ui-group-horizontal"
                  },

                  {
                    "name": "Scrollability direction",
                    "attr": "class",
                    "type": "select",
                    "possibleValues": [
                      { "label": "Vertical", "value": "ys-ui-scrollable-vertically" },
                      { "label": "Horizontal", "value": "ys-ui-scrollable-horizontally" }
                    ],
                    "value": "ys-ui-scrollable-vertically"
                  }

                ]
              }

            ]
          },

          {
            "name": "Box",
            "markup": "<div class=\"\"></div>",
            "parameters": [

              {
                "name": "Size",
                "attr": "class",
                "type": "select",
                "possibleValues": [
                  { "label": "Small", "value": "ys-ui-box--small" },
                  { "label": "Middle", "value": "ys-ui-box--middle" },
                  { "label": "Big", "value": "ys-ui-box--big" },
                  { "label": "Huge", "value": "ys-ui-box--huge" }
                ],
                "value": "ys-ui-box--small"
              }

            ]
          },

          {
            "name": "Grid system",
            "subNodes": [

              {
                "name": "Row",
                "markup": "<div class=\"ys-ui-row\"></div>"
              },

              {
                "name": "Column",
                "markup": "<div class=\"\"></div>",
                "parameters": [

                  {
                    "name": "Size (in columns)",
                    "attr": "class",
                    "type": "select",
                    "possibleValues": [
                      { "label": "Col 1", "value": "ys-ui-col-1" },
                      { "label": "Col 2", "value": "ys-ui-col-2" },
                      { "label": "Col 3", "value": "ys-ui-col-3" },
                      { "label": "Col 4", "value": "ys-ui-col-4" },
                      { "label": "Col 5", "value": "ys-ui-col-5" },
                      { "label": "Col 6", "value": "ys-ui-col-6" },
                      { "label": "Col 7", "value": "ys-ui-col-7" },
                      { "label": "Col 8", "value": "ys-ui-col-8" },
                      { "label": "Col 9", "value": "ys-ui-col-9" },
                      { "label": "Col 10", "value": "ys-ui-col-10" },
                      { "label": "Col 11", "value": "ys-ui-col-11" }
                    ],
                    "value": "ys-ui-col-1"
                  }

                ]
              }

            ]
          }
        ]
      },

      {
        "name": "Components",
        "subNodes": [

          {
            "name": "Date time picker",
            "markup": "<ys-ui-date-time-picker model=\"dt\"></ys-ui-date-time-picker>",
            "parameters": [

              {
                "name": "Minimal date",
                "domAttr": "min-date",
                "type": "free-text",
                "value": ""
              },

              {
                "name": "Maximal date",
                "domAttr": "max-date",
                "type": "free-text",
                "value": ""
              }

            ]
          },

          {
            "name": "List",
            "markup": "<ys-ui-list model=\"dt\"></ys-ui-list>",
            "parameters": [

              {
                "name": "Model",
                "domAttr": "model",
                "type": "free-text",
                "value": ""
              }

            ]
          },

          {
            "name": "List items",
            "subNodes": [

              {
                "name": "Person",
                "markup": "<ys-ui-person-list-item model=\"item\"></ys-ui-person-list-item>"
              },

              {
                "name": "Organization",
                "markup": "<ys-ui-organization-list-item model=\"item\"></ys-ui-organization-list-item>"
              }

            ]
          }

        ]
      },

      {
        "name": "Data access",
        "subNodes" : [

          {
            "name": "Data Fetchers",
            "subNodes": [

              {
                "name": "ES Search",
                "markup": "<ys-ui-es-source model=\"\" max-items=\"\" query=\"\" end-point-url=\"\" searched-index=\"\"></ys-ui-es-source>",
                "parameters": [

                  {
                    "name": "Model",
                    "domAttr": "model",
                    "type": "free-text",
                    "value": "peopleList"
                  },

                  {
                    "name": "Max items",
                    "domAttr": "max-items",
                    "type": "free-text",
                    "value": "maxItems"
                  },

                  {
                    "name": "Query",
                    "domAttr": "query",
                    "type": "free-text",
                    "value": "query"
                  },

                  {
                    "name": "URL",
                    "domAttr": "end-point-url",
                    "type": "free-text",
                    "value": "/api/search"
                  },

                  {
                    "name": "Searched index",
                    "domAttr": "searched-index",
                    "type": "free-text",
                    "value": "persons"
                  }

                ]
              }

            ]
          },

          {
            "name": "Query modifiers",
            "subNodes": [

              {
                "name": "Query builder",
                "markup": "<ys-ui-query-builder model=\"query\"></ys-ui-query-builder>",
                "parameters": [

                  {
                    "name": "Model",
                    "domAttr": "model",
                    "type": "free-text",
                    "value": "query"
                  }

                ]
              },

              {
                "name": "Where",
                "markup": "<ys-ui-where search-key=\"\" search-string=\"\"></ys-ui-query-builder>",
                "parameters": [

                  {
                    "name": "Search Key",
                    "domAttr": "search-key",
                    "type": "free-text",
                    "value": "'searchField'"
                  },

                  {
                    "name": "Search String",
                    "domAttr": "search-string",
                    "type": "free-text",
                    "value": ""
                  }

                ]
              },

              {
                "name": "Paging",
                "markup": "<ys-ui-page page-size=\"query\" page-index=\"\"></ys-ui-page>",
                "parameters": [

                  {
                    "name": "Items per page?",
                    "domAttr": "page-size",
                    "type": "free-text",
                    "value": ""
                  },

                  {
                    "name": "Page index",
                    "domAttr": "page-index",
                    "type": "free-text",
                    "value": ""
                  }

                ]
              },

              {
                "name": "Sorting",
                "markup": "<ys-ui-sort sort-key=\"query\" sort-order=\"\"></ys-ui-sort>",
                "parameters": [

                  {
                    "name": "Sort by ...",
                    "domAttr": "sort-key",
                    "type": "free-text",
                    "value": ""
                  },

                  {
                    "name": "Sort order",
                    "domAttr": "sort-order",
                    "type": "free-text",
                    "value": ""
                  }

                ]
              }

            ]
          }

        ]
      }

    ]
  }
}
